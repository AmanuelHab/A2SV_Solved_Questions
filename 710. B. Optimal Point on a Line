n = int(input())
nums = list(map(int, input().split()))

nums.sort()

prefixSum = [0] * n
prefixSum[0] = nums[0]
for i in range(1, n):
    prefixSum[i] = prefixSum[i - 1] + nums[i]

suffixSum = [0] * n
suffixSum[n - 1] = nums[n - 1]
for i in range(n - 2, -1, -1):
    suffixSum[i] = suffixSum[i + 1] + nums[i]

dist_arr = [0] * n
min_dist = float('inf')
index = 0

for i in range(n):
    dist = 0
    if i > 0:
        dist += abs(nums[i] * (i) - prefixSum[i - 1])
    if n - 1 > i:
        dist += abs(nums[i] * (n - i - 1) - suffixSum[i + 1])

    dist_arr[i] = dist
    if min_dist > dist:
        min_dist = dist
        index = i

print(nums[index])
